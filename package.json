{
  "name": "internnova",
  "description": "The webiste for internnova",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn run check-format && yarn run check-lint && yarn run check-types && yarn run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@clerk/nextjs": "^2.8.0",
    "@elastic/elasticsearch": "^7.16.0",
    "@fontsource/chivo": "^4.5.0",
    "@fontsource/nunito-sans": "^4.5.1",
    "@prisma/client": "^3.8.1",
    "babel-plugin-superjson-next": "^0.4.2",
    "moment": "^2.29.1",
    "next": "latest",
    "next-seo": "^4.28.1",
    "next-transpile-modules": "^9.0.0",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-drive-picker": "^1.1.4",
    "react-hot-toast": "^2.2.0",
    "react-social-icons": "^5.7.0",
    "superjson": "^1.8.0",
    "swr": "^1.1.2",
    "tailwind-glassmorphism": "^1.0.6",
    "tailwindcss-border-gradient-radius": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.2",
    "eslint": "7.0.0",
    "eslint-config-next": "^11.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^2.6.0",
    "husky": "^7.0.4",
    "next-sitemap": "^1.9.12",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.19"
  },
  "pnpm": {
    "overrides": {
      "ansi-regex@>2.1.1 <5.0.1": ">=5.0.1"
