{
  "name": "internnova-web",
  "description": "The webiste for internnova.co",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn run check-format && yarn run check-lint && yarn run check-types && yarn run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.6.1",
    "@clerk/nextjs": "^2.0.3",
    "@prisma/client": "^3.4.0",
    "axios": "^0.23.0",
    "next": "latest",
    "nodemailer": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "autoprefixer": "^10.3.7",
    "eslint": "^7.17.0",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "husky": "^7.0.0",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "prisma": "^3.4.0",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3"
  },
  "engines": {
    "yarn": "1.22.17"
  }
}
