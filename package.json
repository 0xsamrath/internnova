    "axios": "^0.23.0",
    "babel-plugin-superjson-next": "^0.4.2",
    "moment": "^2.29.1",
    "next": "latest",
    "prisma": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.1",
    "react-social-icons": "^5.6.1",
    "superjson": "^1.8.0",
    "swr": "^1.0.1",
    "tailwind-glassmorphism": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
