    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "husky": "^7.0.0",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.15",
    "typescript": "4.5.0"
